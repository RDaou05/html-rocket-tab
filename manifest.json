{
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://kit.fontawesome.com/59e78c517c.js; object-src 'self'",
    "name": "Rocket Tab",
    "version": "1.0.0",
    "description": "Replaces new chrome tab with minimalistic search page",
    "browser_action": {
        "default_icon": "assets/icon.png"
    },
    "icons": {
        "128": "assets/icon.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "scripts/script.js"
            ]
        }
    ],
    "permissions": [
        "storage",
        "tabs",
        "activeTab"
    ]
}
